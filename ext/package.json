{
  "name": "type-tree",
  "displayName": "type-tree",
  "description": "Visualize nested types as a tree",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "repository": {
    "type": "git",
    "url": "https://git.rakuten-it.com/scm/~ts-rodrigo.diaz/type-tree.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "type-tree.create",
        "title": "Type Tree"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "make": "vsce package --baseContentUrl https://git.rakuten-it.com/scm/~ts-rodrigo.diaz/type-tree.git"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@pgrabovets/json-view": "^2.7.5",
    "json-formatter-js": "^2.5.18",
    "jsoneditor": "^10.1.0",
    "picomatch": "^4.0.2",
    "pretty-print-json": "^3.0.3",
    "ts-morph": "^24.0.0"
  }
}
